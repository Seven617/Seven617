            android:label="@string/Pinpad"
            android:screenOrientation="nosensor" />
        <activity
            android:name="cn.eas.national.ldapisample.activity.SerialPortActivity"
            android:label="@string/SerialPort"
            android:screenOrientation="nosensor" />
        <activity
            android:name="cn.eas.national.ldapisample.activity.CameraScannerActivity"
            android:label="@string/CameraScanner"
            android:screenOrientation="nosensor" />
        <activity
            android:name="cn.eas.national.ldapisample.activity.InnerScannerActivity"
