        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cn.eas.national.ldapisample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="cn.eas.national.ldapisample.activity.MagCardActivity"
            android:label="@string/MagCard"
            android:screenOrientation="nosensor" />
        <activity
            android:name="cn.eas.national.ldapisample.activity.ICCpuCardActivity"
            android:label="@string/ICCpuCard"
            android:screenOrientation="nosensor" />
        <activity
            android:name="cn.eas.national.ldapisample.activity.PsamCardActivity"
            android:label="@string/PsamCard"
            android:screenOrientation="nosensor" />
        <activity
            android:name="cn.eas.national.ldapisample.activity.RFCpuCardActivity"
            android:label="@string/RfCpuCard"
            android:screenOrientation="nosensor" ></activity>
        <activity
            android:name="cn.eas.national.ldapisample.activity.MifareCardActivity"
            android:label="@string/MifareCard"
            android:screenOrientation="nosensor" />
        <activity
            android:name="cn.eas.national.ldapisample.activity.IDCardActivity"
            android:label="@string/IDCard"
            android:screenOrientation="nosensor" />
        <activity
            android:name="cn.eas.national.ldapisample.activity.PrinterActivity"
