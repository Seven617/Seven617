            android:name="cn.eas.national.ldapisample.activity.AlgorithmActivity"
            android:label="@string/Algorithm"
            android:screenOrientation="nosensor" />
        <activity
            android:name="cn.eas.national.ldapisample.activity.SystemDeviceActivity"
            android:label="@string/Algorithm"
            android:screenOrientation="nosensor" />

        <activity
            android:name="cn.eas.national.ldapisample.activity.SubscreenActivity"
            android:label="@string/C10SubscreenDevice"
            android:screenOrientation="nosensor"
            android:exported="true"
